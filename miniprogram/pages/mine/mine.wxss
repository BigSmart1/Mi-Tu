/**set.wxss**/
@import "../../lib/weui.wxss";

page{
  height:100%;
  }
  .background {
  width: 100%;
  height: 100%;
  position:fixed; 
  background-size:100% 100%;
  z-index: -1;
  }
.section {
  margin-bottom: 30rpx;
}

.weui-cells__title {
  color: #999;
}

.weui-cells {
  background-color: transparent;
  color: #fff;
  margin-bottom: 40rpx;
}

.weui-cells::before {
  border: 0;
}

.weui-cells::after {
  border: 0;
}
.delete{ 
  width: 50rpx;
  height: 50rpx;
  margin: 10rpx 20rpx 0 0;
}
